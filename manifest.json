{
  "name": "Stripper",
  "description": "Strips all distractions",
  "icons" : {
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"},
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["jquery.js","background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Make this page readable"
  },
  "manifest_version": 2
}
